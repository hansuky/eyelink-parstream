extends ../layout-type01
block style
  link(type='text/css', rel='stylesheet', href='/efsl/assets/css/dc.css') 
  link(type='text/css', rel='stylesheet', href='/efsl/css/chart_style.css')
block script
  script(type='text/javascript', src='/efsl/assets/scripts/d3_dc.js')
  script(type='text/javascript', src='/efsl/assets/scripts/crossfilter.js')
  script(type='text/javascript', src='/efsl/assets/scripts/dc.js')  
  script(type='text/javascript', src='/efsl/assets/scripts/colorbrewer.js')
  script(type='text/javascript', src="/common/scripts/util.js")
  script(type='text/javascript', src='/efsl/scripts/report_power.js')
block content
  .container-fluid
    .page-content
      .breadcrumbs
        h1 Reports_Power
        ol.breadcrumb
          li
            a(href='#') Home
          li
            a(href='#') Reports
          li.active OverView
      .row
        .col-md-12
          .portlet.light.bordered
            .portlet-title
              .caption.font-dark
                span.caption-subject.bold.uppercase.font-blue-sharp Set date and area
                span.caption-helper Date range and select area
              .tools
                a.collapse(href='javascript:;')
                a.config(href='#portlet-config', data-toggle='modal')
                a.reload(href='javascript:;')
                a.fullscreen(href='javascript:;')
                a.remove(href='javascript:;')
            .portlet-body.form
               form.form-horizontal.form-bordered(onsubmit="return false;")
                .form-body
                  .form-group.last
                    label.control-label.col-md-1 Date Range
                    .col-md-4
                      .input-group.input-large.date-picker.input-daterange(data-date='2017-01-01', data-date-format='yyyy-mm-dd')
                        input#sdate.form-control(type='text', name='from' value='')
                        span.input-group-addon to
                        input#edate.form-control(type='text', name='to' value='')
                    label.control-label.col-md-1 Select Area
                    .col-md-6.form-inline
                      select.form-control.input-small.select2me.form-md-line-input(data-placeholder='Select Area')
                        option(value='') Zone ID
                        option(value='') All Area
                        option(value='') AreaID1234
                        option(value='') AreaID5678
                      select.form-control.input-small.select2me.form-md-line-input(data-placeholder='Select Item')
                        option(value='') Node ID
                        option(value='') Node ID
                        option(value='') Item1234
                        option(value='') Item5678
                      button#btn_search.btn.blue
                        i.fa.fa-search Search
      a(href='javascript:dc.filterAll(); dc.renderAll();') Reset All
      .row
        .col-md-6
          .portlet.light
            .portlet-title
              .caption
                span.caption-subject.bold.uppercase.font-green-haze Active Power
                span.caption-helper  Daily Sum
              .actions
                a.btn.btn-default.btn-sm(href='#')
                  i.fa.fa-print
                  |  Print
                a.collapse(href='javascript:;')
                a.btn.btn-icon-only.btn-default.fullscreen(href='#')
            .portlet-body
              .chart
                #powerSum
        .col-md-6
          .portlet.light.bordered
            .portlet-title
              .caption
                span.caption-subject.bold.uppercase.font-green-haze Weekly Amount of Active Power
                span.caption-helper  Min/Max/AvgDay
              .actions
                a.btn.btn-default.btn-sm(href='#')
                  i.fa.fa-print
                  |  Print
                a.collapse(href='javascript:;')
                a.btn.btn-icon-only.btn-default.fullscreen(href='#')
            .portlet-body
              .chart
                #weekPlot
      .row
        .col-md-6
          .portlet.light
            .portlet-title
              .caption
                span.caption-subject.bold.uppercase.font-green-haze Active Power 
                span.caption-helper Time Average
              .actions
                a.btn.btn-default.btn-sm(href='#')
                  i.fa.fa-print
                  |  Print
                a.collapse(href='javascript:;')
                a.btn.btn-icon-only.btn-default.fullscreen(href='#')
            .portlet-body
              .chart
                #timeBar
        .col-md-6
          .portlet.light.bordered
            .portlet-title
              .caption
                span.caption-subject.bold.uppercase.font-green-haze Over Flow Voltage Value
                span.caption-helper Average
              .actions
                a.btn.btn-default.btn-sm(href='#')
                  i.fa.fa-print
                  |  Print
                a.collapse(href='javascript:;')
                a.btn.btn-icon-only.btn-default.fullscreen(href='#')
            .portlet-body
              .chart
                #volLine
    a.page-quick-sidebar-toggler(href='javascript:;')
      i.icon-login
    .page-quick-sidebar-wrapper
      .page-quick-sidebar
        .nav-justified
          ul.nav.nav-tabs.nav-justified
            li.active
              a(href='#quick_sidebar_tab_2', data-toggle='tab')
                | Alerts
                span.badge.badge-success 7
            li
              a(href='#quick_sidebar_tab_1', data-toggle='tab')
                | Users
                span.badge.badge-danger 2
            li.dropdown
              a.dropdown-toggle(href='javascript:;', data-toggle='dropdown')
                | More
                i.fa.fa-angle-down
              ul.dropdown-menu.pull-right(role='menu')
                li
                  a(href='#quick_sidebar_tab_3', data-toggle='tab')
                    i.fa.fa-bell
                    |  Alerts
                li
                  a(href='#quick_sidebar_tab_3', data-toggle='tab')
                    i.fa.fa-bookmark
                    |  Notifications
                li
                  a(href='#quick_sidebar_tab_3', data-toggle='tab')
                    i.fa.fa-comments
                    |  Activities
                li.divider
                li

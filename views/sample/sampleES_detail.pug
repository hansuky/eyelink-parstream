block content
  body.body-popup
    .wrapper
      header.page-header
      .container-fluid
        .content-popup
          .popup-tit-box           
           .row
             .col-md-12
                h1 EyeLink
          .row
            .col-md-12
              .portlet.light.bordered(style='height: 730px;')                
                .portlet-body.form                                 
                  table#sample_2.table.table-striped.table-bordered.table-hover(style='text-align:center;')
                      thead
                        tr
                          th(style='text-align:center;') No
                          th(style='text-align:center;') Start Time
                          th(style='text-align:center;') Path
                          th(style='text-align:center;') Res.(ms)
                          th(style='text-align:center;') Response
                          th(style='text-align:center;') Client IP
                      tbody
                        each data in list
                          tr.odd.gradeX
                            td #{data.no}
                            td #{data.timestamp}
                            td(style='text-align:left;') #{data.request}
                            td #{data.responsetime}
                            td #{data.response}
                            td #{data.clientip}

    <query id='selectBottleneckList'><![CDATA[
      {
        "index" : #index#,
        "type" : #type#,
        "body" : {
          "size" : 10000,
          "sort" : {"timestamp" : { "order" : "desc" }},
          "query": {
            "bool": {
              "filter": { "range": { 
                "timestamp" : { "gte": #start#, "lte" : #end# } } },
              "must" : [{ "match": { "agentId" : #server#  } },
                        { "match": { "alarmType" : #list#  } }]
            }
          }
        }
      }
    ]]></query>
